name: dockercompose5646958317986446612
services:
  caixa.api:
    build:
      context: E:\Renan\PROJETOS\Caixa
      dockerfile: Caixa.Api/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
    image: caixaapi
    networks:
      default: null
    ports:
      - mode: ingress
        target: 8080
